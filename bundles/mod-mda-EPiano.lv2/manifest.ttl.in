@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix mda:  <http://moddevices.com/plugins/mda/> .
@prefix mdap: <http://moddevices.com/plugins/mda/presets#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

modplugins:EPiano
    a lv2:Plugin ;
    rdfs:seeAlso <EPiano.ttl> ;
    lv2:binary <EPiano@LIB_EXT@> .

mdapset:EPiano-default
    a pset:Preset ;
    lv2:appliesTo mda:EPiano ;
    rdfs:seeAlso <EPiano-presets.ttl> .

mdapset:EPiano-bright
    a pset:Preset ;
    lv2:appliesTo mda:EPiano ;
    rdfs:seeAlso <EPiano-presets.ttl> .

mdapset:EPiano-mellow
    a pset:Preset ;
    lv2:appliesTo mda:EPiano ;
    rdfs:seeAlso <EPiano-presets.ttl> .

mdapset:EPiano-autopan
    a pset:Preset ;
    lv2:appliesTo mda:EPiano ;
    rdfs:seeAlso <EPiano-presets.ttl> .

mdapset:EPiano-tremolo
    a pset:Preset ;
    lv2:appliesTo mda:EPiano ;
    rdfs:seeAlso <EPiano-presets.ttl> .

